@model Tuple<Invent.Models.Entity.Configuration.ApiGeneralEntity, Invent.Models.Entity.Configuration.FlipkartEntity, Invent.Models.Entity.Configuration.AmazonEntity>
@{
    ViewBag.Title = "Channel";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style type="text/css">
    .modal-backdrop {
        position: inherit !important;
    }
</style>
<script src="~/assets/module_scripts/Setting/channel.js"></script>
@{
    Html.RenderPartial("../Shared/PartialView/Setting/_TopMenu");
}

<div class="body-section">
    <div class="action-area">
        <div class="pull-right base-top2">
            <ul class="report-action">
                <li>
                    <input type="text" id="filter-text-box" class="form-control" placeholder="Search this list..." oninput="onFilterTextBoxChanged()" />
                </li>
                <li><a href="@Url.Action("Add_Channel","Setting")" class="btn btn-primary"><i class="fa fa-plus"></i>Add Channel</a></li>
                <li>
                    <a href="javascript:void(0)"><i class="fa fa-download"></i> <i class="fa fa-caret-down"></i></a>
                </li>
            </ul>
        </div>
    </div>
    <div class="data-grid-section">
        <div id="channel-grid" class="grdstyle ag-theme-balham" style="height:420px;"></div>
    </div>
</div>


@* Confgure Amazon Channel *@
<div class="modal fade" id="amazon-channel">
    <div class="modal-dialog modal-dialog-centered modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Integration</h4>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>

            <!-- Modal body -->
            <div class="modal-body">
                <div class="row">
                    <div class="channel-info col-lg-3 col-xs-12 col-md-3 col-sm-3">
                        <img src="~/assets/img/channel-logo/S_Amazon.png" />
                        <div class="summary-div" id="summary-div">
                            <div class="ch-summary">
                                <span class="chttl">Channel Summary</span>
                                <hr />
                                <ul>
                                    <li><span>Revenue <span class="pull-right">0</span></span></li>
                                    <li><span>Orders <span class="pull-right">0</span></span></li>
                                    <li><span>Orders Completed <span class="pull-right">0</span></span></li>
                                    <li><span>Failed Orders <span class="pull-right">0</span></span></li>
                                    <li><span>Failed Inventory Updates <span class="pull-right">0</span></span></li>
                                </ul>
                            </div>
                            <div class="ch-summary">
                                <span class="chttl">Orders Sync <a href="javascript:void(0)"><i class="fa fa-sync"></i></a></span>
                                <hr />
                                <ul>
                                    <li><span>Orders <span class="pull-right">0</span></span></li>
                                    <li><span>Failed Orders <span class="pull-right">0</span></span></li>
                                </ul>
                            </div>
                            <div class="ch-summary">
                                <span class="chttl">Product Sync <a href="javascript:void(0)"><i class="fa fa-sync"></i></a></span>
                                <hr />
                                <ul>
                                    <li><span>Product Live <span class="pull-right">0</span></span></li>
                                    <li><span>Product Not Live <span class="pull-right">0</span></span></li>
                                    <li><span>Product Mapped <span class="pull-right">0</span></span></li>
                                    <li><span>Products UnMapped <span class="pull-right">0</span></span></li>
                                    <li><span>New Product Updated <span class="pull-right">0</span></span></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="channel-add col-lg-9 col-xs-12 col-md-9 col-sm-9">
                        <ul class="nav nav-tabs">
                            <li class="active2"><a data-toggle="tab" href="#setting">Setting</a></li>
                            <li><a data-toggle="tab" class="disabled" href="#connector">Connector</a></li>
                        </ul>
                        <div class="tab-content">
                            <div id="setting" class="tab-pane fade in active show">
                                @using (Ajax.BeginForm("SaveChannelDetails", "Configuration",
new AjaxOptions
{
    HttpMethod = "POST",
    OnSuccess = "OnChannelSuccess",
    OnFailure = "OnChannelFailure",
    LoadingElementId = "progress"
}))
                                {
                                    @Html.HiddenFor(model => model.Item1.ChannelId, new { @id = "AzChId" });
                                    
                                    <div class="header-ttl">
                                        <span class="subheader">General Information</span>
                                        <p>Follow these instruction to know how to add Channel <button id="amazonSave" type="submit" class="btn btn-success">Update & Next</button>  </p>
                                    </div>
                                    <div class="gen-form">
                                        <ul>
                                            <li>
                                                <label class="private-label">Channel Name*</label>
                                                @Html.TextBoxFor(model => model.Item1.ChannelName, new { @id = "AZChannelName", @class = "private-form-control" })
                                                @Html.ValidationMessageFor(model => model.Item1.ChannelName)
                                                @*<input type="text" class="private-form-control" />*@
                                            </li>
                                            <li>
                                                <label class="private-label">Leadger Name (Optional)</label>
                                                @Html.TextBoxFor(model => model.Item1.LeadgerName, new { @id = "AZLedgerName", @class = "private-form-control" })
                                                @Html.ValidationMessageFor(model => model.Item1.LeadgerName)
                                                @*<input type="text" class="private-form-control" />*@
                                            </li>
                                        </ul>
                                    </div>
                                    <div class="header-ttl">
                                        <span class="subheader">Order</span>
                                        <p>This section help us to configure important parameters for order and sync.</p>
                                    </div>
                                    <div class="gen-form">
                                        <div class="checkbox">
                                            <label class="AZOrder">
                                                @Html.CheckBoxFor(model => model.Item1.OrderSync, new { @id = "AZOrderSync", @data_toggle = "toggle" })
                                                @Html.ValidationMessageFor(model => model.Item1.OrderSync)
                                                @*<input type="checkbox" data-toggle="toggle">*@
                                                Sync (Optional)
                                            </label>
                                        </div>
                                    </div>
                                    <div class="header-ttl">
                                        <span class="subheader">Inventory</span>
                                        <p>Once the inentory is ON, We will update inventory on channel.</p>
                                    </div>
                                    <div class="gen-form">
                                        <div class="checkbox">
                                            <label class="AZInvent">
                                                @Html.CheckBoxFor(model => model.Item1.InventorySync, new { @id = "AZInventorySync", @data_toggle = "toggle" })
                                                @Html.ValidationMessageFor(model => model.Item1.InventorySync)
                                                @*<input type="checkbox" data-toggle="toggle">*@
                                                Sync (Optional)
                                            </label>
                                        </div>
                                    </div>
                                }
                            </div>
                            <div id="connector" class="tab-pane fade">
                                @using (Ajax.BeginForm("SaveChannelAmazonDetails", "Configuration",
new AjaxOptions
{
    HttpMethod = "POST",
    OnSuccess = "OnChannelApiSuccess",
    OnFailure = "OnChannelApiFailure",
    LoadingElementId = "progress"
}))
                                {
                                    @Html.HiddenFor(model => model.Item3.Flag, new { @id = "AZFlag" });
                                    <div class="header-ttl">
                                        <span class="subheader">Amazon MWS API </span>
                                        <p>Provide API credentials that you get when you authorized you as a developer <button class="btn btn-success"><i class="fa fa-plug"></i> Re-Connect</button></p>
                                    </div>
                                    <div class="gen-form">
                                        <ul>
                                            <li>
                                                <label class="private-label">Seller Id</label>
                                                @Html.TextBoxFor(model => model.Item3.SellerId, new { @id = "AZSellerId", @class = "private-form-control" })
                                                @Html.ValidationMessageFor(model => model.Item3.SellerId)
                                                @*<input type="text" class="private-form-control" />*@
                                            </li>
                                            <li>
                                                <label class="private-label">Marketplace Id</label>
                                                @Html.TextBoxFor(model => model.Item3.MarketplaceId, new { @id = "AZMarketplaceId", @class = "private-form-control" })
                                                @Html.ValidationMessageFor(model => model.Item3.MarketplaceId)
                                                @*<input type="text" class="private-form-control" />*@
                                            </li>
                                            <li>
                                                <label class="private-label">AWS Access Key Id</label>
                                                @Html.TextBoxFor(model => model.Item3.AccessKey, new { @id = "AZAccessKey", @class = "private-form-control" })
                                                @Html.ValidationMessageFor(model => model.Item3.AccessKey)
                                                @*<input type="text" class="private-form-control" />*@
                                            </li>
                                            <li>
                                                <label class="private-label">Secret Key</label>
                                                @Html.TextBoxFor(model => model.Item3.SecretKey, new { @id = "AZSecretKey", @class = "private-form-control" })
                                                @Html.ValidationMessageFor(model => model.Item3.SecretKey)
                                                @*<input type="text" class="private-form-control" />*@
                                            </li>
                                            <li>
                                                <label class="private-label">MWS Auth Token</label>
                                                @Html.TextBoxFor(model => model.Item3.AuthToken, new { @id = "AZAuthToken", @class = "private-form-control" })
                                                @Html.ValidationMessageFor(model => model.Item3.AuthToken)
                                                @*<input type="text" class="private-form-control" />*@
                                            </li>
                                        </ul>
                                    </div>
                                    <div class="header-ttl">
                                        <span class="subheader">Seller Credentials</span>
                                        <p>Provide Credentials that you use to login into your seller panel.</p>
                                    </div>
                                    <div class="gen-form">
                                        <ul>
                                            <li>
                                                <label class="private-label">Username</label>
                                                @Html.TextBoxFor(model => model.Item3.Username, new { @id = "AZUserName", @class = "private-form-control" })
                                                @Html.ValidationMessageFor(model => model.Item3.Username)
                                                @*<input type="text" class="private-form-control" />*@
                                            </li>
                                            <li>
                                                <label class="private-label">Password</label>
                                                @Html.PasswordFor(model => model.Item3.Password, new { @id = "AZPassword", @class = "private-form-control" })
                                                @Html.ValidationMessageFor(model => model.Item3.Password)
                                            </li>
                                        </ul>
                                    </div>
                                }
                            </div>

                        </div>
                    </div>
                </div>

            </div>

        </div>
    </div>
</div>
@* Configure Amazon End *@

