@model Tuple<Invent.Models.Entity.Configuration.ChannelGeneralDetailsEntity, Invent.Models.Entity.Configuration.FlipkartEntity, Invent.Models.Entity.Configuration.AmazonEntity>
@{
    Layout = null;
    Invent.Models.Entity.User.UserEntity user = (Invent.Models.Entity.User.UserEntity)HttpContext.Current.Session["UserEntity"];
}
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Step 2</title>
    <link href="~/assets/css/all.css" rel="stylesheet" />
    <link href="~/assets/css/bootstrap.min.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Nunito:300,400,600,700,800" rel="stylesheet">
    <link href="~/assets/css/default.css" rel="stylesheet" />
    <link href="~/assets/css/jquery.toast.min.css" rel="stylesheet" />
</head>
<body>
    @if (user != null)
    {
        <div class="configure-modal-two">
            <div class="configure-header-two" id="configure-menu">
                <ul>
                    <li>
                        <a step="1" href="javascript:Void(0)" class="config-disabled">
                            <i class="fa fa-landmark"></i>
                            1. Add Facility
                        </a>
                    </li>
                    <li>
                        <a step="2" href="javascript:Void(0)" class="config-active">
                            <i class="fa fa-tv"></i>
                            2. Add Channel
                        </a>
                    </li>
                    <li>
                        <a step="3" href="javascript:Void(0)" class="config-disabled">
                            <i class="fa fa-file-invoice"></i>
                            3. Invoice Configuration
                        </a>
                    </li>
                </ul>
            </div>
            <div class="configure-body-two" style="background:#fff;">
                <div class="container">
                    <div id="addchannel" class="addchannel">
                        <span class="subheader">Marketplace </span>
                        <ul>
                            <li>
                                <a href="javascript:void(0)" data-toggle="modal" data-target="#amazon-channel">
                                    <img src="~/assets/img/channel-logo/S_Amazon.png" title="Amazon" />
                                    <div class="channel-btn">
                                        <span>Amazon</span>
                                    </div>
                                </a>
                            </li>
                            <li>
                                <a href="javascript:void(0)" data-toggle="modal" data-target="#paytm-channel">
                                    <img src="~/assets/img/channel-logo/S_Paytm.png" title="Paytm" />
                                    <div class="channel-btn">
                                        <span>Paytm</span>
                                    </div>
                                </a>
                            </li>
                            <li>
                                <a href="javascript:void(0)" data-toggle="modal" data-target="#flipkart-channel">
                                    <img src="~/assets/img/channel-logo/S_Flipkart.png" title="Flipkart" />
                                    <div class="channel-btn">
                                        <span>Flipkart</span>
                                    </div>
                                </a>
                            </li>
                            <li>
                                <a href="javascript:void(0)" data-toggle="modal" data-target="#shopcluse-channel">
                                    <img src="~/assets/img/channel-logo/S_Shopclues.png" title="Shopclues" />
                                    <div class="channel-btn">
                                        <span>ShopClues</span>
                                    </div>
                                </a>
                            </li>
                            <li>
                                <a href="javascript:void(0)" data-toggle="modal" data-target="#snapdeal-channel">
                                    <img src="~/assets/img/channel-logo/S_Snapdeal.png" title="Snapdeal" />
                                    <div class="channel-btn">
                                        <span>Snapdeal</span>
                                    </div>
                                </a>
                            </li>
                            <li>
                                <a href="javascript:void(0)" data-toggle="modal" data-target="#myntra-channel">
                                    <img src="~/assets/img/channel-logo/S_Ebay.png" title="Ebay" />
                                    <div class="channel-btn">
                                        <span>Ebay</span>
                                    </div>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>

        </div>
    }
    @* Confgure Amazon Channel *@
    <div class="modal fade" id="amazon-channel">
        <div class="modal-dialog modal-dialog-centered modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title">Integration</h4>
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                </div>

                <!-- Modal body -->
                <div class="modal-body">
                    <div class="row">
                        <div class="channel-info col-lg-3 col-xs-12 col-md-3 col-sm-3">
                            <img src="~/assets/img/channel-logo/b-amazon.png" />
                            <div class="step-follow">
                                <ol>
                                    <li>Go to <a href="http://developer.amazonservices.in">http://developer.amazonservices.in</a></li>
                                    <li>Click the Sign up for MWS button.</li>
                                    <li>Log into your Amazon seller account.</li>
                                    <li>Select the following option: <b>"I want to access my Amazon seller account with MWS"</b> and click next.</li>
                                    <li>Accept the Amazon MWS License Agreement.</li>
                                    <li>Copy the credential and paste it</li>
                                </ol>
                            </div>
                        </div>
                        <div class="channel-add col-lg-9 col-xs-12 col-md-9 col-sm-9">
                            <ul class="nav nav-tabs">
                                <li class="active2"><a data-toggle="tab" href="#setting">Setting</a></li>
                                <li><a data-toggle="tab" href="#connector">Connector</a></li>
                            </ul>
                            <div class="tab-content">
                                <div id="setting" class="tab-pane fade in active show">
                                    @using (Ajax.BeginForm("SaveChannelDetails", "Configuration",
new AjaxOptions
{
    HttpMethod = "POST",
    OnSuccess = "OnChannelSuccess",
    OnFailure = "OnChannelFailure",
    LoadingElementId = "progress"
}))
                                    {
                                        <div class="header-ttl">
                                            <span class="subheader">General Information</span>
                                            <p>Follow these instruction to know how to add Channel <button id="amazonSave" type="submit" class="btn btn-success">Save & Next</button>  </p>
                                        </div>
                                        <div class="gen-form">
                                            <ul>
                                                <li>
                                                    <label class="private-label">Channel Name*</label>
                                                    @Html.TextBoxFor(model => model.Item1.ChannelName, new { @class = "private-form-control" })
                                                    @Html.ValidationMessageFor(model => model.Item1.ChannelName)
                                                    @*<input type="text" class="private-form-control" />*@
                                                </li>
                                                <li>
                                                    <label class="private-label">Leadger Name (Optional)</label>
                                                    @Html.TextBoxFor(model => model.Item1.LeadgerName, new { @class = "private-form-control" })
                                                    @Html.ValidationMessageFor(model => model.Item1.LeadgerName)
                                                    @*<input type="text" class="private-form-control" />*@
                                                </li>
                                            </ul>
                                        </div>
                                        <div class="header-ttl">
                                            <span class="subheader">Order</span>
                                            <p>This section help us to configure important parameters for order and sync.</p>
                                        </div>
                                        <div class="gen-form">
                                            <div class="checkbox">
                                                <label>
                                                    @Html.CheckBoxFor(model => model.Item1.OrderSync, new { @data_toggle = "toggle" })
                                                    @Html.ValidationMessageFor(model => model.Item1.OrderSync)
                                                    @*<input type="checkbox" data-toggle="toggle">*@
                                                    Sync (Optional)
                                                </label>
                                            </div>
                                        </div>
                                        <div class="header-ttl">
                                            <span class="subheader">Inventory</span>
                                            <p>Once the inentory is ON, We will update inventory on channel.</p>
                                        </div>
                                        <div class="gen-form">
                                            <div class="checkbox">
                                                <label>
                                                    @Html.CheckBoxFor(model => model.Item1.InventorySync, new { @data_toggle = "toggle" })
                                                    @Html.ValidationMessageFor(model => model.Item1.InventorySync)
                                                    @*<input type="checkbox" data-toggle="toggle">*@
                                                    Sync (Optional)
                                                </label>
                                            </div>
                                        </div>
                                    }
                                </div>
                                <div id="connector" class="tab-pane fade">
                                    @using (Ajax.BeginForm("SaveChannelAmazonDetails", "Configuration",
new AjaxOptions
{
    HttpMethod = "POST",
    OnSuccess = "OnChannelApiSuccess",
    OnFailure = "OnChannelApiFailure",
    LoadingElementId = "progress"
}))
                                    {
                                        <div class="header-ttl">
                                            <span class="subheader">Amazon MWS API </span>
                                            <p>Provide API credentials that you get when you authorized you as a developer <button id="amaConnector" class="btn btn-success"><i class="fa fa-plug"></i> Connect</button></p>
                                        </div>
                                        <div class="gen-form">
                                            <ul>
                                                <li>
                                                    <label class="private-label">Seller Id</label>
                                                    @Html.TextBoxFor(model => model.Item3.SellerId, new { @class = "private-form-control" })
                                                    @Html.ValidationMessageFor(model => model.Item3.SellerId)
                                                    @*<input type="text" class="private-form-control" />*@
                                                </li>
                                                <li>
                                                    <label class="private-label">Marketplace Id</label>
                                                    @Html.TextBoxFor(model => model.Item3.MarketplaceId, new { @class = "private-form-control" })
                                                    @Html.ValidationMessageFor(model => model.Item3.MarketplaceId)
                                                    @*<input type="text" class="private-form-control" />*@
                                                </li>
                                                <li>
                                                    <label class="private-label">AWS Access Key Id</label>
                                                    @Html.TextBoxFor(model => model.Item3.AccessKey, new { @class = "private-form-control" })
                                                    @Html.ValidationMessageFor(model => model.Item3.AccessKey)
                                                    @*<input type="text" class="private-form-control" />*@
                                                </li>
                                                <li>
                                                    <label class="private-label">Secret Key</label>
                                                    @Html.TextBoxFor(model => model.Item3.SecretKey, new { @class = "private-form-control" })
                                                    @Html.ValidationMessageFor(model => model.Item3.SecretKey)
                                                    @*<input type="text" class="private-form-control" />*@
                                                </li>
                                                <li>
                                                    <label class="private-label">MWS Auth Token</label>
                                                    @Html.TextBoxFor(model => model.Item3.AuthToken, new { @class = "private-form-control" })
                                                    @Html.ValidationMessageFor(model => model.Item3.AuthToken)
                                                    @*<input type="text" class="private-form-control" />*@
                                                </li>
                                            </ul>
                                        </div>
                                        <div class="header-ttl">
                                            <span class="subheader">Seller Credentials</span>
                                            <p>Provide Credentials that you use to login into your seller panel.</p>
                                        </div>
                                        <div class="gen-form">
                                            <ul>
                                                <li>
                                                    <label class="private-label">Username</label>
                                                    @Html.TextBoxFor(model => model.Item3.Username, new { @class = "private-form-control" })
                                                    @Html.ValidationMessageFor(model => model.Item3.Username)
                                                    @*<input type="text" class="private-form-control" />*@
                                                </li>
                                                <li>
                                                    <label class="private-label">Password</label>
                                                    @Html.PasswordFor(model => model.Item3.Password, new { @class = "private-form-control" })
                                                    @Html.ValidationMessageFor(model => model.Item3.Password)
                                                    <input type="text" class="private-form-control" />
                                                </li>
                                            </ul>
                                        </div>
                                    }
                                </div>

                            </div>
                        </div>
                    </div>

                </div>

            </div>
        </div>
    </div>
    @* Configure Amazon End *@

